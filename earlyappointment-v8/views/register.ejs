<%- include('./partials/header') %> 




<form action="/register" method="POST">

             <div class="container  ">
             <label for="exampleInputEmail1" class="heading d-flex justify-content-center mt-4  mb-3">Register Form</label>
                  <div class="form-group">
                    <label for="exampleInputEmail1">Email address</label>
                    <input type="email" class="form-control" name="username" id="exampleInputEmail1" aria-describedby="emailHelp">
                    <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
            </div>
           <div class="form-group">
                  <label for="exampleInputPassword1">Password</label>
                  <input type="password" name="password" class="form-control" id="exampleInputPassword1">
          </div>
         <button type="submit" class="btn btn-primary  ">Submit</button>
</div>
</form>

